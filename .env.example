# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# Nova Act SDK Configuration (optional)
NOVA_ACT_API_KEY=your_nova_act_api_key_here
NOVA_ACT_HEADLESS=true
NOVA_ACT_CHROME_CHANNEL=chrome

# Application Settings
DEBUG=false
LOG_LEVEL=INFO
REPORT_OUTPUT_DIR=./reports
MAX_DAYS_HISTORY=90

# Dashboard Security (if using web interface)
DASHBOARD_USERNAME=admin
DASHBOARD_PASSWORD=secure_password_here

# Service-Specific Settings
ENABLE_RESOURCE_CLEANUP=true
ENABLE_COST_ALERTS=true
COST_ALERT_THRESHOLD=100.00
